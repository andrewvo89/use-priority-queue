  "version": "1.0.2",
  "packageManager": "yarn@4.7.0",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "license": "MIT",
  "repository": {
    "url": "git+https://github.com/andrewvo89/use-priority-queue.git"
  },
  "keywords": [
    "react",
    "hook",
    "priority",
    "queue",
    "data",
    "structure",
    "typescript"
  ],
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
